<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashBoard.Home.CheckOut.GuestCheckout">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout26"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorPrimary2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:fontFamily="@font/roboto_bold"
                    android:gravity="center"
                    android:text="@string/checkout"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="TextContrastCheck" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/guest_checkout_ImV_backBtn"
                    android:layout_width="40dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_left_arrow24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ImageContrastCheck" />

                <View
                    android:id="@+id/view7"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="#E7E7E7"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView25"
                android:layout_width="205dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto"
                android:paddingStart="20dp"
                android:paddingEnd="2dp"
                android:text="@string/guestCheckout_delivery_details"
                android:textColor="@color/colorPrimary2"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="@+id/constraintLayout26"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout26"
                tools:ignore="TextContrastCheck" />

            <TextView
                android:id="@+id/textView42"
                android:layout_width="205dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto"
                android:paddingStart="20dp"
                android:paddingEnd="2dp"
                android:text="@string/checkout_payment_details"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/constraintLayout26"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout26" />

            <View
                android:id="@+id/view8"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:layout_marginTop="4dp"
                android:background="@color/colorPrimary2"
                app:layout_constraintEnd_toEndOf="@+id/textView25"
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/textView25" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view8">

                <TextView
                    android:id="@+id/textView41"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary2"
                    android:fontFamily="@font/roboto"
                    android:padding="10dp"
                    android:text="@string/guestCheckout_addressDetails"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/Guest_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_name"
                    android:inputType="textPersonName|text"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView41"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guest_phone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_phone"
                    android:inputType="phone"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/Guest_name"
                    app:layout_constraintStart_toStartOf="@+id/Guest_name"
                    app:layout_constraintTop_toBottomOf="@+id/Guest_name"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guest_email"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_email"
                    android:inputType="textWebEmailAddress"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/guest_phone"
                    app:layout_constraintStart_toStartOf="@+id/guest_phone"
                    app:layout_constraintTop_toBottomOf="@+id/guest_phone"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guest_address"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_address"
                    android:inputType="textPostalAddress"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:paddingEnd="2dp"
                    android:paddingBottom="5dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/guest_email"
                    app:layout_constraintStart_toStartOf="@+id/guest_email"
                    app:layout_constraintTop_toBottomOf="@+id/guest_email"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guest_apt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_apt"
                    android:inputType="text"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/guest_address"
                    app:layout_constraintStart_toStartOf="@+id/guest_address"
                    app:layout_constraintTop_toBottomOf="@+id/guest_address"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guest_buzzer"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_buzzer"
                    android:inputType="text"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/guest_apt"
                    app:layout_constraintStart_toStartOf="@+id/guest_apt"
                    app:layout_constraintTop_toBottomOf="@+id/guest_apt"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guest_extraInfo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_extra_info"
                    android:inputType="text"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/guest_buzzer"
                    app:layout_constraintStart_toStartOf="@+id/guest_buzzer"
                    app:layout_constraintTop_toBottomOf="@+id/guest_buzzer"
                    tools:ignore="TextContrastCheck" />

                <CheckBox
                    android:id="@+id/guest_business_radioBtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    app:layout_constraintStart_toStartOf="@+id/guest_extraInfo"
                    app:layout_constraintTop_toBottomOf="@+id/guest_extraInfo"
                    tools:ignore="DuplicateSpeakableTextCheck" />

                <TextView
                    android:id="@+id/editText11"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto"
                    android:gravity="center_vertical"
                    android:minHeight="48dp"
                    android:text="@string/guestCheckout_business"
                    android:textColor="@color/text"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/guest_business_radioBtn"
                    app:layout_constraintEnd_toEndOf="@+id/guest_extraInfo"
                    app:layout_constraintStart_toEndOf="@+id/guest_business_radioBtn"
                    app:layout_constraintTop_toTopOf="@+id/guest_business_radioBtn" />

                <CheckBox
                    android:id="@+id/guest_hotel_radioBtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    app:layout_constraintStart_toStartOf="@+id/guest_business_radioBtn"
                    app:layout_constraintTop_toBottomOf="@+id/guest_business_radioBtn" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:fontFamily="@font/roboto"
                    android:gravity="center_vertical"
                    android:text="@string/guestCheckout_motel"
                    android:textColor="@color/text"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/guest_hotel_radioBtn"
                    app:layout_constraintEnd_toEndOf="@+id/editText11"
                    app:layout_constraintStart_toEndOf="@+id/guest_hotel_radioBtn"
                    app:layout_constraintTop_toTopOf="@+id/guest_hotel_radioBtn"
                    tools:ignore="TouchTargetSizeCheck" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout23"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout21">

                <TextView
                    android:id="@+id/textView115"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary2"
                    android:fontFamily="@font/roboto"
                    android:padding="@dimen/_10sdp"
                    android:text="@string/guestCheckout_text1"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="TextContrastCheck" />


                <TextView
                    android:id="@+id/guestCheckOut_TV_days"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginStart="16dp"
                    android:fontFamily="@font/roboto"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingEnd="5dp"
                    android:text="@string/guestCheckout_today"
                    android:textColor="@color/text"
                    android:textSize="14sp"
                    app:drawableEndCompat="@drawable/ic_arrow_down_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/textView115"
                    app:layout_constraintTop_toBottomOf="@+id/textView115"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/guestCheckOut_TV_time"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="16dp"
                    android:fontFamily="@font/roboto"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingEnd="5dp"
                    android:text="@string/guestCheckout_morning"
                    android:textColor="@color/text"
                    android:textSize="14sp"
                    app:drawableEndCompat="@drawable/ic_arrow_down_24"
                    app:layout_constraintBottom_toBottomOf="@+id/guestCheckOut_TV_days"
                    app:layout_constraintStart_toEndOf="@+id/guestCheckOut_TV_days"
                    app:layout_constraintTop_toTopOf="@+id/guestCheckOut_TV_days"
                    app:layout_constraintVertical_bias="0.0" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout31"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout23">

                <TextView
                    android:id="@+id/textView118"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary2"
                    android:fontFamily="@font/roboto"
                    android:padding="@dimen/_10sdp"
                    android:text="@string/guestCheckout_text2"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/checkOut_TV_extraInfo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="8dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:fontFamily="@font/roboto"
                    android:hint="@string/guestCheckout_text4"
                    android:inputType="text"
                    android:minHeight="48dp"
                    android:padding="10dp"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView118"
                    app:layout_constraintStart_toStartOf="@+id/textView118"
                    app:layout_constraintTop_toBottomOf="@+id/textView118" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout32"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout31">

                <TextView
                    android:id="@+id/textView119"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary2"
                    android:fontFamily="@font/roboto"
                    android:padding="@dimen/_10sdp"
                    android:text="@string/guestCheckout_text3"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/guestCheck_editText_pass"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:autofillHints=""
                    android:background="@drawable/profile_border"
                    android:inputType="textPassword|numberPassword"
                    android:minHeight="48dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="2dp"
                    android:text="@string/guestCheckout_password"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView119" />

                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkBoxBtn_TC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="@+id/guestCheck_editText_pass"
                    app:layout_constraintTop_toBottomOf="@+id/guestCheck_editText_pass"
                    tools:ignore="TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:fontFamily="@font/roboto"
                    android:padding="2dp"
                    android:text="@string/guestCheckout_i_agree"
                    android:textColor="@color/text"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/checkBoxBtn_TC"
                    app:layout_constraintStart_toEndOf="@+id/checkBoxBtn_TC"
                    app:layout_constraintTop_toTopOf="@+id/checkBoxBtn_TC" />

                <TextView
                    android:id="@+id/guestCheckOut_TC_tV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto"
                    android:padding="2dp"
                    android:text="@string/guestCheckout_terms_conditions"
                    android:textColor="@color/colorPrimary2"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/textView17"
                    app:layout_constraintStart_toEndOf="@+id/textView17"
                    app:layout_constraintTop_toTopOf="@+id/textView17"
                    tools:ignore="TextContrastCheck" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/guestCheckout_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/button_corner"
                android:fontFamily="@font/roboto"
                android:text="@string/guestCheckout_checkout"
                android:textColor="@color/white"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout32" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>